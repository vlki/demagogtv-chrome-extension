{
  "name": "demagog-chrome-extension",
  "version": "0.1.0",
  "scripts": {
    "start": "NODE_ENV=development webpack --config webpack.config.js --watch",
    "build": "npm run build-webpack && npm run build-compress",
    "build-webpack": "NODE_ENV=production webpack --config webpack.config.js",
    "build-compress": "node scripts/compress.js",
    "generate-signing-key": "crx keygen",
    "clean": "rimraf build/ dev/ *.crx"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "copy-webpack-plugin": "^4.4.1",
    "crx": "^3.2.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^3.1.6",
    "webpack": "^3.11.0",
    "webpack-chrome-extension-reloader": "^0.7.0"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "rimraf": "^2.6.2"
  }
}
